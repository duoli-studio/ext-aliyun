{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": [
		"framework",
		"laravel"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=7.0",
		"laravel/framework": "5.5.*",
		"intervention/image": "2.*",
		"laravelcollective/html": "5.5.*",
		"mews/purifier": "~2.0",
		"mews/captcha": "~2.0",
		"nesbot/carbon": "~1.14",
		"predis/predis": "~1.1",
		"doctrine/dbal": "^2.5",
		"guzzlehttp/guzzle": "6.*",
		"php-curl-class/php-curl-class": "~4.8",
		"phpoffice/phpexcel": "~1.8.0",
		"maatwebsite/excel": "~2.1",
		"tymon/jwt-auth": "1.0.*@dev",
		"jenssegers/date": "^3.2",
		"webonyx/graphql-php": "0.11.*",
		"tucker-eric/eloquentfilter": "^1.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~6.0",
		"phpspec/phpspec": "~2.1",
		"itsgoingd/clockwork": "~2.0",
		"barryvdh/laravel-ide-helper": "~2.4"
	},
	"autoload": {
		"classmap": [

		],
		"psr-4": {
			"App\\": "app/",
			"Poppy\\Framework\\": "framework/src",
			"System\\": "modules/system/src",
			"Order\\": "modules/order/src"
		},
		"files": [
			"framework/src/Support/functions.php"
		]
	},
	"autoload-dev": {
		"classmap": [

		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate",
			"php artisan ide-helper:meta"
		],
		"post-root-package-install": [
			"php -r \"copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://packagist.phpcomposer.com"
		},
		{
			"packagist": false
		}
	]
}
