{
	"name": "poppy/framework",
	"description": "A framework for laravel module development.",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Mark Zhao",
			"email": "zhaody901@126.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"jenssegers/date": "^3.3",
		"laravelcollective/html": "~5.5",
		"webonyx/graphql-php": "0.11.*"
	},
	"autoload": {
		"psr-4": {
			"Poppy\\Framework\\": "src/"
		},
		"files": [
			"src/Support/functions.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Poppy\\Framework\\Tests\\": "tests/"
		}
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
